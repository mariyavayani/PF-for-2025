//Q1
#include <stdio.h>
#include <stdlib.h>

int countFrequency(int size, int arr[]);
int main() {
	int n;
	printf("Enter the size of array: ");
	scanf("%d", &n);
	int array1[n];
	for (int i = 0; i < n; i++) {
        array1[i] = rand() % 11; 
        printf("%d ", array1[i]);
    }
    printf("\n");
    countFrequency(n,array1);
	return 0;
}

int countFrequency(int size, int arr[]) {
	int freq[11]={0};
	for (int i=0;i<size;i++) {
		freq[arr[i]]++;
	}
	for (int i=0;i<=10;i++) {
		if (freq[i] > 0)
		printf("The number %d occurs %d times.\n", i, freq[i]);
	}
}

//Q2
#include <stdio.h>
void assignedPosition();

int main() {
	assignedPosition();
	return 0;
}

void assignedPosition() {
	float marks, experience;
	printf("Enter your test marks: ");
	scanf("%f", &marks);
	if (marks >= 50 && marks<60) {
		printf("You are selected for the post of trainee engineer.");
	}
	else if (marks>=60) {
		printf("Enter experience: ");
		scanf("%f", &experience);
		if (marks >= 70 && experience >= 2) {
            printf("You are selected for the post of Associate Developer.\n");
        } 
        else if (marks >= 60 && experience >= 1) {
            printf("You are selected for the post of Assistant Developer.\n");
        } 
        else {
            printf("You do not meet the experience criteria for these positions.\n");
        }
    } 
    else {
        printf("You did not qualify for any position.\n");
    }
}

//Q3
#include <stdio.h>
float volumeCall(float x, float y);
int main() {
	float h,a,result;
	printf("Enter height: ");
	scanf("%f", &h);
	printf("Enter base: ");
	scanf("%f", &a);
	result = volumeCall(h,a);
	printf("The volume is: %.1f", result);
	return 0;
}

float volumeCall(float x, float y) {
	return (y*y*x)/3.0;
}
