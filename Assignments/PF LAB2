//Q4
#include <stdio.h>
int main(void) {
	printf("*********\t   ***  \t   *   \t            *\n");
	printf("*\t*\t *     * \t  ***\t\t  *   *   \n");
	printf("*\t*\t*\t*\t *****\t\t *     *   \n");
	printf("*\t*\t*\t*\t   *\t \t*       *\n");
	printf("*\t*\t*\t*\t   *           *         * \n");
	printf("*\t*\t*\t*\t   *\t\t*       *\n");
	printf("*\t*\t*\t*\t   *\t\t *     *\n");
	printf("*\t*\t *     *\t   *\t\t  *   *\n");
	printf("*********\t   ***\t           *\t\t    *\n");
}
