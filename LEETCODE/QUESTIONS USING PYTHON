867. TRANSPOSE OF A MATRIX
lass Solution(object):
    def transpose(self, matrix):
        """
        :type matrix: List[List[int]]
        :rtype: List[List[int]]
        """
        rows = len(matrix)
        cols = len(matrix[0])
        transposed=[]
        for i in range(cols):
            row = []
            for j in range(rows):
                row.append(0)
            transposed.append(row)

        for i in range(rows):
            for j in range(cols):
                transposed[j][i] = matrix[i][j]

        return transposed


1. Two Sum
class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        for i in range(len(nums)):
            for j in range(i+1,len(nums)):
                if (nums[i] + nums[j]) == target:
                    return [i,j]


2. Palindrome Number
class Solution(object):
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        
        if str(x) == str(x)[::-1]:
            return True
        else:
            return False


13. Roman to Integer
class Solution(object):
    def romanToInt(self, s):
        total = 0

        def value(ch):
            if ch == 'I': return 1
            elif ch == 'V': return 5
            elif ch == 'X': return 10
            elif ch == 'L': return 50
            elif ch == 'C': return 100
            elif ch == 'D': return 500
            elif ch == 'M': return 1000
            else: return 0

        for i in range(len(s) - 1):
            if value(s[i]) < value(s[i + 1]):
                total -= value(s[i])
            else:
                total += value(s[i])

        total += value(s[-1])
        return total


14. Longest common prefix
class Solution(object):
    def longestCommonPrefix(self, strs):
        """
        :type strs: List[str]
        :rtype: str
        """
        prefix = strs[0]
        for s in strs[1:]:
            while not s.startswith(prefix):
                prefix = prefix[:-1]
            if not prefix:
                return ""
        
        return prefix


349. Intersection of two arrays
class Solution(object):
    def intersection(self, nums1, nums2):
        """
        :type nums1: List[int]
        :type nums2: List[int]
        :rtype: List[int]
        """
        results=[]
        for num in nums1:
            if num in nums2 and num not in results:
                results.append(num)
                
        return results


2956. Find common elements between two arrays
class Solution(object):
    def findIntersectionValues(self, nums1, nums2):
        """
        :type nums1: List[int]
        :type nums2: List[int]
        :rtype: List[int]
        """
        answer1 = 0
        answer2 = 0
        
        for x in nums1:
            if x in nums2:
                answer1+=1

        for y in nums2:
            if y in nums1:
                answer2+=1


        return [answer1,answer2]


1662. Check if two string arrays are equivalent
class Solution(object):
    def arrayStringsAreEqual(self, word1, word2):
        """
        :type word1: List[str]
        :type word2: List[str]
        :rtype: bool
        """
        
        if  ''.join(word1) == ''.join(word2):
            return True
        else:
            return False

1480. Running sum of 1d array
class Solution(object):
    def runningSum(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """
        runningSum=[]
        sum=0

        for i in range(len(nums)):
            sum += nums[i]
            runningSum.append(sum)
        
        return runningSum


2215. Find the difference of two arrays
class Solution(object):
    def findDifference(self, nums1, nums2):
        """
        :type nums1: List[int]
        :type nums2: List[int]
        :rtype: List[List[int]]
        """
        answer = [[],[]]
        for i in nums1:
            if i not in nums2:
                if i not in answer[0]:
                    answer[0].append(i)

        
        for j in nums2:
            if j not in nums1:
                if j not in answer[1]:
                    answer[1].append(j)

        return answer

3069. Distribute elements into two arrays I
class Solution(object):
    def resultArray(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """
        arr1 = [nums[0]]
        arr2 = [nums[1]]
        
        
        for i in range(2,len(nums)):
            if arr1[-1] > arr2[-1]:
                arr1.append(nums[i])
            else:
                arr2.append(nums[i])
        
        return arr1 + arr2

2319. Check if matrix is X-Matrix
class Solution(object):
    def checkXMatrix(self, grid):
        """
        :type grid: List[List[int]]
        :rtype: bool
        """
        flag1 = True
        flag2 = True
        for i in range(len(grid)):
            for j in range(len(grid)):
                if i==j or i==len(grid)-1-j:
                    if grid[i][j] == 0:
                        flag1 = False


                else if i!=j and j!=len(grid)-1-i:
                    if grid[i][j] != 0:
                        flag2 = False

        if flag1 and flag2:
            return True
        else:
            return False


2729. Check if the number is fascinating
class Solution(object):
    def isFascinating(self, n):
        """
        :type n: int
        :rtype: bool
        """
        temp = str(n) + str(2*n) + str(3*n)
        
        flag = True
        if len(temp) != 9:
            return False

        
        if '0' in temp:
            return False
        if '1' not in temp:
            flag = False
        if '2' not in temp:
            flag = False
        if '3' not in temp:
            flag = False
        if '4' not in temp:
            flag = False
        if '5' not in temp:
            flag = False
        if '6' not in temp:
            flag = False
        if '7' not in temp:
            flag = False
        if '8' not in temp:
            flag = False
        if '9' not in temp:
            flag = False

        return flag


1232. Check if it is a straight line
class Solution(object):
    def checkStraightLine(self, coordinates):
        """
        :type coordinates: List[List[int]]
        :rtype: bool
        """
        x0, y0 = coordinates[0]
        x1, y1 = coordinates[1]

        for i in range(2, len(coordinates)):
            x, y = coordinates[i]
            if (x - x0) * (y1 - y0) != (y - y0) * (x1 - x0):
                return False

        return True

2124. Check if all a's appear before all b's
class Solution(object):
    def checkString(self, s):
        """
        :type s: str
        :rtype: bool
        """
        for i in range(len(s)-1):
            if s[i] == 'b' and s[i+1] == 'a':
                return False
        return True

3232. Find if digit game can be won
class Solution(object):
    def canAliceWin(self, nums):
        """
        :type nums: List[int]
        :rtype: bool
        """
        double_sum=0
        single_sum=0
        for i in range(len(nums)):
            if nums[i]/10 >= 1:
                double_sum+=nums[i]
            else:
                single_sum+=nums[i]

        if double_sum != single_sum:
            return True
        else:
            return False


1880. Check if word equals summation of two words
class Solution(object):
    def isSumEqual(self, firstWord, secondWord, targetWord):
        """
        :type firstWord: str
        :type secondWord: str
        :type targetWord: str
        :rtype: bool
        """
        s=[]
        a=[]
        t=[]
        for i in firstWord:
            if i=='a':
                s.append('0')
            if i=='b':
                s.append('1')
            if i=='c':
                s.append('2')
            if i=='d':
                s.append('3')
            if i=='e':
                s.append('4')
            if i=='f':
                s.append('5')
            if i=='g':
                s.append('6')
            if i=='h':
                s.append('7')
            if i=='i':
                s.append('8')
            if i=='j':
                s.append('9')

        for i in secondWord:
            if i=='a':
                a.append('0')
            if i=='b':
                a.append('1')
            if i=='c':
                a.append('2')
            if i=='d':
                a.append('3')
            if i=='e':
                a.append('4')
            if i=='f':
                a.append('5')
            if i=='g':
                a.append('6')
            if i=='h':
                a.append('7')
            if i=='i':
                a.append('8')
            if i=='j':
                a.append('9')


        for i in targetWord:
            if i=='a':
                t.append('0')
            if i=='b':
                t.append('1')
            if i=='c':
                t.append('2')
            if i=='d':
                t.append('3')
            if i=='e':
                t.append('4')
            if i=='f':
                t.append('5')
            if i=='g':
                t.append('6')
            if i=='h':
                t.append('7')
            if i=='i':
                t.append('8')
            if i=='j':
                t.append('9')

       
        s=''.join(s)
        a=''.join(a)
        t=''.join(t)
        if int(a)+int(s)==int(t):
            return True
        else:
            return False


69.Sqrt(x)
class Solution(object):
    def mySqrt(self, x):
        """
        :type x: int
        :rtype: int
        """
        return int(math.sqrt(x))


70. Climbing stairs
class Solution(object):
    def climbStairs(self, n):
        """
        :type n: int
        :rtype: int
        """
        a = 0
        b = 1
        for i in range(n):
            c = a + b
            a = b
            b = c
        
        return b

125. Valid palindrome
class Solution(object):
    def isPalindrome(self, s):
        """
        :type s: str
        :rtype: bool
        """
        a=""
        for x in s:
            if x.isalnum():
                a+=x.lower()
        s=s.lower()
        if a == a[::-1]:
            return True
        else:
            return False


217. Contains duplicate
class Solution(object):
    def containsDuplicate(self, nums):
        """
        :type nums: List[int]
        :rtype: bool
        """
        seen = set()
        for n in nums:
            if n in seen:
                return True
            seen.add(n)
        return False


231. power of two
class Solution(object):
    def isPowerOfTwo(self, n):
        """
        :type n: int
        :rtype: bool
        """
        if n>0 and not (n & (n-1)):
            return True
        else:
            return False
